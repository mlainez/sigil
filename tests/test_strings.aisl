(mod test
  (fn strlen ((s string)) -> i32
    (ret (call string_length s)))
  
  (test-spec strlen
    (case "length of Hello"
      (input "Hello")
      (expect 5))
    (case "length of World"
      (input "World")
      (expect 5))
    (case "length of empty"
      (input "")
      (expect 0))
    (case "length of long string"
      (input "Hello World!")
      (expect 12)))
  
  (meta-note "Tests string_length function"))
