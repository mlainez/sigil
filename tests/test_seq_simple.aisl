(mod test
  (fn return_last_of_three ((a i32) (b i32) (c i32)) -> i32
    (ret c))
  
  (test-spec return_last_of_three
    (case "sequence 1 2 3"
      (input 1 2 3)
      (expect 3))
    (case "sequence 10 20 30"
      (input 10 20 30)
      (expect 30)))
  
  (fn sum_three ((a i32) (b i32) (c i32)) -> i32
    (ret (call add (call add a b) c)))
  
  (test-spec sum_three
    (case "sum 1 2 3"
      (input 1 2 3)
      (expect 6))
    (case "sum 10 20 30"
      (input 10 20 30)
      (expect 60)))
  
  (meta-note "Tests sequence operations with multiple values"))