(module test_debug_return
  (fn add_two a int b int -> int
    (set result int (add a b))
    (print "[Function returning: ")
    (print result)
    (print "]\n")
    (ret result))
  
  (test-spec add_two
    (case "debug test"
      (input 2 3)
      (expect 6)))
  
  (meta-note "Debug test to see what function returns"))
