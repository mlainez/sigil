(module demo
  (fn find_max_of_five v1 int v2 int v3 int v4 int v5 int -> int
    (set max int v1)
    (if (gt v2 max)
      (set max int v2))
    (if (gt v3 max)
      (set max int v3))
    (if (gt v4 max)
      (set max int v4))
    (if (gt v5 max)
      (set max int v5))
    (ret max))
  
  (test-spec find_max_of_five
    (case "max is 15"
      (input 5 12 3 8 15)
      (expect 15))
    (case "max is first"
      (input 20 10 5 3 1)
      (expect 20))
    (case "max is middle"
      (input 5 10 25 15 8)
      (expect 25))
    (case "all equal"
      (input 7 7 7 7 7)
      (expect 7)))
  
  (meta-note "Tests finding maximum value among five integers using if statements"))