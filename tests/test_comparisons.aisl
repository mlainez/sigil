(mod test
  (fn eq_i32 ((a i32) (b i32)) -> bool
    (ret (call op_eq_i32 a b)))
  
  (test-spec eq_i32
    (case "equal values"
      (input 5 5)
      (expect 1))
    (case "different values"
      (input 5 10)
      (expect 0)))
  
  (fn ne_i32 ((a i32) (b i32)) -> bool
    (ret (call op_ne_i32 a b)))
  
  (test-spec ne_i32
    (case "different values"
      (input 5 10)
      (expect 1))
    (case "equal values"
      (input 5 5)
      (expect 0)))
  
  (fn lt_i32 ((a i32) (b i32)) -> bool
    (ret (call op_lt_i32 a b)))
  
  (test-spec lt_i32
    (case "less than"
      (input 3 10)
      (expect 1))
    (case "greater than"
      (input 10 3)
      (expect 0))
    (case "equal values"
      (input 5 5)
      (expect 0)))
  
  (fn gt_i32 ((a i32) (b i32)) -> bool
    (ret (call op_gt_i32 a b)))
  
  (test-spec gt_i32
    (case "greater than"
      (input 10 3)
      (expect 1))
    (case "less than"
      (input 3 10)
      (expect 0))
    (case "equal values"
      (input 5 5)
      (expect 0)))
  
  (fn le_i32 ((a i32) (b i32)) -> bool
    (ret (call op_le_i32 a b)))
  
  (test-spec le_i32
    (case "equal values"
      (input 5 5)
      (expect 1))
    (case "less than"
      (input 3 10)
      (expect 1))
    (case "greater than"
      (input 10 3)
      (expect 0)))
  
  (fn ge_i32 ((a i32) (b i32)) -> bool
    (ret (call op_ge_i32 a b)))
  
  (test-spec ge_i32
    (case "equal values"
      (input 5 5)
      (expect 1))
    (case "greater than"
      (input 10 3)
      (expect 1))
    (case "less than"
      (input 3 10)
      (expect 0)))
  
  (meta-note "Tests all i32 comparison operators: eq, ne, lt, gt, le, ge"))
