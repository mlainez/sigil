(mod test_polymorphic_print
  (fn test_print_string -> string
    (set s string "hello")
    (ret s))
  
  (fn test_print_i32 -> int
    (set x int 42)
    (ret x))
  
  (fn test_print_i64 -> int
    (set x int 9999999999)
    (ret x))
  
  (fn test_print_f32 -> float
    (set x float 3.14)
    (ret x))
  
  (fn test_print_f64 -> float
    (set x float 2.718)
    (ret x))
  
  (fn test_print_bool_true -> bool
    (ret true))
  
  (fn test_print_bool_false -> bool
    (ret false))
  
  (fn test_print_comparison -> bool
    (ret (call lt 5 10)))
  
  (test-spec test_print_string
    (case "returns string value"
      (input)
      (expect "hello")))
  
  (test-spec test_print_i32
    (case "returns int value"
      (input)
      (expect 42)))
  
  (test-spec test_print_i64
    (case "returns int value"
      (input)
      (expect 9999999999)))
  
  (test-spec test_print_f32
    (case "returns float value"
      (input)
      (expect 3.14)))
  
  (test-spec test_print_f64
    (case "returns float value"
      (input)
      (expect 2.718)))
  
  (test-spec test_print_bool_true
    (case "returns true"
      (input)
      (expect true)))
  
  (test-spec test_print_bool_false
    (case "returns false"
      (input)
      (expect false)))
  
  (test-spec test_print_comparison
    (case "returns comparison result"
      (input)
      (expect true)))
  
  (meta-note "Tests polymorphic print dispatch for all primitive types: string, int, int, float, float, bool, and nested comparison expressions"))
