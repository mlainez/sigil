(module test_math_abs
  (import math)
  
  (fn abs_negative -> int
    (ret (abs -5)))
  
  (fn abs_positive -> int
    (ret (abs 10)))
  
  (test-spec abs_negative
    (case "returns absolute value of negative number"
      (input)
      (expect 5)))
  
  (test-spec abs_positive
    (case "returns absolute value of positive number"
      (input)
      (expect 10)))
  
  (meta-note "Tests math.abs function from math module"))
