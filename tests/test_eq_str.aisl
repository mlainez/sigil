(module test_eq_strings
  (fn test_string_eq s1 string s2 string -> bool
    (ret (call string_eq s1 s2)))
  
  (test-spec test_string_eq
    (case "returns true for identical strings"
      (input "object" "object")
      (expect true))
    (case "returns false for different strings"
      (input "object" "array")
      (expect false))
    (case "returns true for empty strings"
      (input "" "")
      (expect true))
    (case "returns false for empty vs non-empty"
      (input "" "text")
      (expect false)))
  
  (meta-note "Tests string_eq for comparing string equality"))
