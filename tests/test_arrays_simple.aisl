(mod test
  (fn new_array_length size int -> int
    (ret (call array_length (call array_new size))))
  
  (test-spec new_array_length
    (case "create array size 10"
      (input 10)
      (expect 0))
    (case "create array size 0"
      (input 0)
      (expect 0))
    (case "create array size 100"
      (input 100)
      (expect 0)))
  
  (meta-note "Tests array_new creates empty arrays with array_length returning 0"))