(module test
  (fn max_i64 x int y int -> int
    (set result int y)
    (if (gt x y)
      (set result int x))
    (ret result))
  
  (test-spec max_i64
    (case "first larger"
      (input 20 10)
      (expect 20))
    (case "second larger"
      (input 10 20)
      (expect 20))
    (case "equal values"
      (input 15 15)
      (expect 15))
    (case "both large"
      (input 100 50)
      (expect 100))
    (case "negative values"
      (input -5 -10)
      (expect -5)))
  
  (meta-note "Tests conditional logic for finding maximum value using if statement"))
