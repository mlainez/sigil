(mod test
  (fn eq_i64 ((a i64) (b i64)) -> bool
    (ret (call op_eq_i64 a b)))
  
  (test-spec eq_i64
    (case "equal large numbers"
      (input 100 100)
      (expect 1))
    (case "not equal large numbers"
      (input 1000000000000 2000000000000)
      (expect 0)))
  
  (fn lt_i64 ((a i64) (b i64)) -> bool
    (ret (call op_lt_i64 a b)))
  
  (test-spec lt_i64
    (case "less than"
      (input 50 100)
      (expect 1))
    (case "not less than"
      (input 100 50)
      (expect 0)))
  
  (fn gt_i64 ((a i64) (b i64)) -> bool
    (ret (call op_gt_i64 a b)))
  
  (test-spec gt_i64
    (case "greater than"
      (input 100 50)
      (expect 1))
    (case "not greater than"
      (input 50 100)
      (expect 0)))
  
  (meta-note "Tests i64 comparison operations for large integers"))
