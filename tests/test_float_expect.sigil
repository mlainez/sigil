(module test_float_expect
  (fn divide a float b float -> float
    (ret (div a b)))
  
  (test-spec divide
    (case "divides evenly"
      (input 10.0 2.0)
      (expect 5.0))
    (case "divides to get 4"
      (input 20.0 5.0)
      (expect 4.0)))
  
  (meta-note "Tests float division with expected float values"))
