(module test_import_string
  (import string_utils)
  
  (fn get_length -> int
    (set text string "hello world")
    (set len int (string_length text))
    (ret len))
  
  (test-spec get_length
    (case "gets length of hello world string"
      (input)
      (expect 11)))
  
  (meta-note "Tests string_length with string_utils import"))
