(module test
  (fn get_first val1 int val2 int -> int
    (ret (array_get (array_push (array_push (array_new) val1) val2) 0)))
  
  (test-spec get_first
    (case "get first element"
      (input 42 100)
      (expect 42))
    (case "get first different values"
      (input 99 77)
      (expect 99)))
  
  (fn get_second val1 int val2 int -> int
    (ret (array_get (array_push (array_push (array_new) val1) val2) 1)))
  
  (test-spec get_second
    (case "get second element"
      (input 42 100)
      (expect 100))
    (case "get second different values"
      (input 99 77)
      (expect 77)))
  
  (meta-note "Tests array_get retrieves elements at specified indices"))