(module simple_tests

  (fn add a int b int -> int
    (ret (add a b)))
  
  (test-spec add
    (case "simple addition"
      (input 2 3)
      (expect 5))
    
    (case "zero addition"
      (input 0 5)
      (expect 5))
    
    (case "negative numbers"
      (input -2 3)
      (expect 1)))
  
  (meta-note "This is a simple test example")
  
  (fn multiply x int y int -> int
    (ret (mul x y)))
)
