(module test
  (fn eq_i64 a int b int -> bool
    (ret (eq a b)))
  
  (test-spec eq_i64
    (case "equal large numbers"
      (input 1000000000000 1000000000000)
      (expect true)))
  
  (fn ne_i64 a int b int -> bool
    (ret (ne a b)))
  
  (test-spec ne_i64
    (case "not equal large numbers"
      (input 1000000000000 2000000000000)
      (expect true)))
  
  (fn lt_i64 a int b int -> bool
    (ret (lt a b)))
  
  (test-spec lt_i64
    (case "less than large numbers"
      (input 1000000000000 2000000000000)
      (expect true)))
  
  (fn gt_i64 a int b int -> bool
    (ret (gt a b)))
  
  (test-spec gt_i64
    (case "greater than large numbers"
      (input 2000000000000 1000000000000)
      (expect true)))
  
  (fn le_i64 a int b int -> bool
    (ret (le a b)))
  
  (test-spec le_i64
    (case "less or equal int"
      (input 1000000000000 1000000000000)
      (expect true)))
  
  (fn ge_i64 a int b int -> bool
    (ret (ge a b)))
  
  (test-spec ge_i64
    (case "greater or equal int"
      (input 2000000000000 1000000000000)
      (expect true)))
  
  (meta-note "Tests all comparison operators for int, float, and float types"))
