(module test_decimal_fail
  (fn add_decimals a decimal b decimal -> decimal
    (ret (add a b)))
  
  (test-spec add_decimals
    (case "should fail - wrong expected"
      (input (cast_float_decimal 10.5) (cast_float_decimal 5.25))
      (expect "20.0"))
    (case "should pass"
      (input (cast_float_decimal 10.5) (cast_float_decimal 5.25))
      (expect "15.75")))
  
  (meta-note "Test decimal with intentional failure"))
