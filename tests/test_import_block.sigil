(module test_import_block
  (import math)
  
  (fn get_string_length -> int
    (set text string "hello")
    (set len int (string_length text))
    (ret len))
  
  (test-spec get_string_length
    (case "gets length of hello string"
      (input)
      (expect 5)))
  
  (meta-note "Tests that string_length is a built-in operation (no string_utils import needed)"))
