(module process
  (fn spawn command string args array -> string
    (ret (process_spawn command args)))
  
  (fn exec command string args array -> int
    (ret (process_exec command args)))
  
  (fn wait proc string -> int
    (ret (process_wait proc)))
  
  (fn kill proc string signal int -> bool
    (ret (process_kill proc signal)))
  
  (fn pipe -> array
    (ret (process_pipe)))
  
  (fn read proc string -> string
    (ret (process_read proc)))
  
  (fn write proc string data string -> bool
    (ret (process_write proc data))))
